{
  "manifest_version": 2,

  "name": "Glancify",
  "description": "This extensions collects your News Feeds and Notfications and displays it on the New Tab",
  "version": "1.0",

  "icons": {
    "16": "16.png",
    "32": "32.png",
    "64": "64.png",
    "128": "128.png"
  },

  "background": {
    "page": "custom_newtab.html"
  },

  "permissions": [
    "notifications",
    "https://twitter.com/i/notifications"
  ],


  "chrome_url_overrides": {
    "newtab": "custom_newtab.html"
  }

}
